{
  "name": "partial.lenses",
  "version": "5.3.0",
  "description": "Partial lenses, isomorphisms and traversals for manipulating JSON",
  "main": "lib/partial.lenses.js",
  "scripts": {
    "bench": "NODE_ENV=production node bench/bench.js",
    "dist": "babel src --source-maps inline --out-dir lib",
    "docs-bundle": "NODE_ENV=dev browserify -r ./lib/partial.lenses:partial.lenses -r ramda -r moment -r infestines -r babel-polyfill -g envify -g uglifyify > docs/bundle.js",
    "docs": "./scripts/generate-docs",
    "lint": "eslint src test",
    "prepublish": "npm run lint && npm run test && npm run dist",
    "test": "nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calmm-js/partial.lenses.git"
  },
  "keywords": [
    "json",
    "lens",
    "isomorphism",
    "traversal",
    "partial"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/calmm-js/partial.lenses/issues"
  },
  "homepage": "https://github.com/calmm-js/partial.lenses#readme",
  "dependencies": {
    "infestines": "^0.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "benchmark": "^2.1.2",
    "browserify": "^13.1.1",
    "envify": "^4.0.0",
    "eslint": "^3.11.1",
    "marked": "^0.3.6",
    "mocha": "^3.2.0",
    "moment": "^2.17.1",
    "nyc": "^10.0.0",
    "ramda": "^0.22.1",
    "ramda-lens": "^0.1.1",
    "uglifyify": "^3.0.4"
  }
}
